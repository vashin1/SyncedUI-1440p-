		<Frames>
			<Button name="EQL3_Tracker_MinimizeButton" hidden="true">
				<Size>
					<AbsDimension x="32" y="32"/>
				</Size>
				<Anchors>
					<Anchor point="TOPRIGHT" relativeTo="EQL3_QuestWatchFrame" relativePoint="TOPRIGHT">
						<Offset>
							<AbsDimension x="2" y="-2"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnClick>
						EQL3_Toggle_Tracker();
					</OnClick>
				</Scripts>
				<NormalTexture file="Interface\Addons\EQL3\Images\MinimizeButton-Up"/>
				<PushedTexture file="Interface\Addons\EQL3\Images\MinimizeButton-Down"/>
				<HighlightTexture file="Interface\Buttons\UI-Panel-MinimizeButton-Highlight" AlphaMode="ADD"/>
			</Button>
		</Frames>
		
		<Scripts>
			<OnEvent>
				EQL_QuestTracker_OnEvent(event);
			</OnEvent>
			<OnLoad>
				EQL_QuestTracker_OnLoad();
			</OnLoad>
			<OnDragStart>
				 EQL3_QuestWatchFrame:StartMoving();
				 EQL3_Temp.movingWatchFrame = true;
			</OnDragStart>
			<OnDragStop>
				 EQL3_QuestWatchFrame:StopMovingOrSizing();
				 EQL3_Temp.movingWatchFrame = nil;
				 QuestWatchFrame_LockCornerForGrowth();
			</OnDragStop>
		</Scripts>
	</Frame>
	
	<Frame name="EQL3_QuestWatchFrame_Debug" toplevel="true" parent="UIParent" hidden="false" frameStrata="BACKGROUND" enableMouse="false" movable="false">
		<Size>
			<AbsDimension x="1" y="1"/>
		</Size>
		<Anchors>
			<Anchor point="TOPRIGHT">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Scripts>	
			<OnUpdate>
				EQL3_QuestWatchFrame_OnUpdate(arg1);
			</OnUpdate>
		</Scripts>
	</Frame>
	
	<Frame name="QuestWatchFrameBackdrop" parent="EQL3_QuestWatchFrame" frameStrata="BACKGROUND" setAllPoints="true">
		<Backdrop bgFile="Interface\Tooltips\UI-Tooltip-Background" edgeFile="Interface\Tooltips\UI-Tooltip-Border" tile="true">
			<EdgeSize>
				<AbsValue val="16"/>
			</EdgeSize>
			<TileSize>
				<AbsValue val="16"/>
			</TileSize>
			<BackgroundInsets>
				<AbsInset left="5" right="5" top="5" bottom="5"/>
			</BackgroundInsets>
		</Backdrop>
		<Scripts>
			<OnLoad>
				this:SetFrameLevel("1");
				this:SetBackdropBorderColor(
					TOOLTIP_DEFAULT_BACKGROUND_COLOR.r,
					TOOLTIP_DEFAULT_BACKGROUND_COLOR.g,
					TOOLTIP_DEFAULT_BACKGROUND_COLOR.b);
				this:SetBackdropColor(
					TOOLTIP_DEFAULT_BACKGROUND_COLOR.r,
					TOOLTIP_DEFAULT_BACKGROUND_COLOR.g,
					TOOLTIP_DEFAULT_BACKGROUND_COLOR.b);
				this:SetAlpha(0);
			</OnLoad>
		</Scripts>
	</Frame>
	
